<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.alibaba.push.android.demo">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- Application Settings -->
    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/AppTheme">

        <!-- 请填写你自己的- appKey EMASDemo空间 23793506 -->
        <meta-data
            android:name="com.alibaba.app.appkey"
            android:value="XXX" />
        <!-- 请填写你自己的appSecret -->
        <meta-data
            android:name="com.alibaba.app.appsecret"
            android:value="XXX" />
        <!--huawei厂商通道参数配置-->
        <meta-data
            android:name="com.huawei.hms.client.appid"
            android:value="appid=XXXXX" />
        <!--vivo厂商通道参数配置-->
        <meta-data
            android:name="com.vivo.push.api_key"
            android:value="XXXXX" />
        <meta-data
            android:name="com.vivo.push.app_id"
            android:value="XXXXX" />
        <!--荣耀厂商通道配置-->
        <meta-data
            android:name="com.hihonor.push.app_id"
            android:value="XXX" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:launchMode="singleInstance"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AllLabelActivity"
            android:screenOrientation="portrait"
            android:exported="false" />

        <activity android:name=".AddTagActivity"
            android:theme="@style/TransparentStyle"
            android:screenOrientation="portrait"
            android:exported="false" />

        <activity android:name=".AliasListActivity"
            android:theme="@style/TransparentStyle"
            android:screenOrientation="portrait"
            android:exported="false" />

        <activity
            android:name="com.alibaba.push.android.demo.PopupPushActivity"
            android:process="@string/aliyun_accs_target_process"
            android:exported="true"
            android:launchMode="singleInstance">
            <!-- 假设 ThirdPushPopupActivity 是用于接收厂商通道推送数据的辅助弹窗  -->
            <!-- scheme 配置开始  -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="${applicationId}"
                    android:path="/thirdpush"
                    android:scheme="agoo" />
            </intent-filter>
            <!-- scheme 配置结束  -->
        </activity>

        <activity
            android:name="com.alibaba.push.android.demo.ThirdPushReceiveActivity"
            android:process="@string/aliyun_accs_target_process"
            android:exported="true"
            android:launchMode="singleInstance">

            <!-- scheme 配置开始  -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="${applicationId}"
                    android:path="/thirdpush"
                    android:scheme="agoo" />
            </intent-filter>
            <!-- scheme 配置结束  -->
        </activity>


        <receiver
            android:name=".MyMessageReceiver"
            android:process="@string/aliyun_accs_target_process"
            android:exported="false">
            <intent-filter>
                <action android:name="com.alibaba.push2.action.NOTIFICATION_OPENED" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.alibaba.push2.action.NOTIFICATION_REMOVED" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.alibaba.sdk.android.push.RECEIVE" />
            </intent-filter>
        </receiver>

        <service
            android:name=".MyMessageIntentService"
            android:process="@string/aliyun_accs_target_process"
            android:exported="false">
            <intent-filter>
                <action android:name="com.alibaba.push2.action.NOTIFICATION_OPENED" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.alibaba.push2.action.NOTIFICATION_REMOVED" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.alibaba.sdk.android.push.RECEIVE" />
            </intent-filter>
        </service>

    </application>

</manifest>